{
    "name": "@mashroom/mashroom-portal-demo-angular-app",
    "description": "Simple Angular 7 based app that integrates into the Portal",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.0.92",
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.13.1",
        "@angular/cli": "^7.3.1",
        "@angular/common": "^7.2.5",
        "@angular/compiler": "^7.2.5",
        "@angular/compiler-cli": "^7.2.5",
        "@angular/core": "^7.2.5",
        "@angular/forms": "^7.2.5",
        "@angular/http": "^7.2.5",
        "@angular/language-service": "^7.2.5",
        "@angular/platform-browser": "^7.2.5",
        "@angular/platform-browser-dynamic": "^7.2.5",
        "core-js": "^2.6.5",
        "rxjs": "^6.4.0",
        "tslint": "^5.12.1",
        "typescript": "3.2.4",
        "zone.js": "^0.8.29"
    },
    "scripts": {
        "dev": "ng serve",
        "build": "ng build",
        "lint": "ng lint"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Angular App",
                "title": "Demo Angular App",
                "type": "portal-app",
                "bootstrap": "startAngularDemoApp",
                "category": "Demo",
                "resources": {
                    "js": [
                        "runtime.js",
                        "polyfills.js",
                        "styles.js",
                        "vendor.js",
                        "main.js"
                    ],
                    "css": []
                },
                "defaultConfig": {
                    "resourcesRoot": "./dist",
                    "restProxies": {},
                    "appConfig": {
                        "firstName": "Adam"
                    }
                }
            }
        ]
    }
}
