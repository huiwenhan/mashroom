{
    "parser": "babel-eslint",
    "plugins": [
        "flowtype"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:flowtype/recommended"
    ],
    "env": {
        "node": false,
        "browser": true,
        "es6": true
    },
    "globals": {
        "global": true
    },
    "rules": {
        "no-console": "off",
        "quotes": ["error", "single", { "allowTemplateLiterals": true }]
    }
}
