{
    "name": "@mashroom/mashroom-session-provider-filestore",
    "description": "Mashroom Server session file store provider",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.2.0",
    "files": [
        "dist/**",
        "type-definitions/*"
    ],
    "dependencies": {
        "session-file-store": "^1.3.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@mashroom/mashroom": "1.2.0",
        "@mashroom/mashroom-session": "1.2.0",
        "eslint": "^6.6.0",
        "express": "^4.17.1"
    },
    "scripts": {
        "lint": "eslint src --fix",
        "build": "babel src -d dist"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Session Filestore Provider",
                "type": "session-store-provider",
                "bootstrap": "./dist/mashroom-bootstrap.js",
                "defaultConfig": {
                    "path": "./data/sessions"
                }
            }
        ]
    }
}
