{
    "name": "@mashroom/mashroom-portal-demo-load-dynamically-app",
    "description": "Mashroom Portal Demo Load apps dynamically",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.1.0",
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@babel/cli": "^7.6.0",
        "@mashroom/mashroom-portal": "1.1.0",
        "eslint": "^6.4.0",
        "jest": "^24.9.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.8",
        "webpack-dev-server": "^3.8.0"
    },
    "scripts": {
        "lint": "eslint src",
        "dev": "webpack-dev-server --hot --mode development",
        "build": "webpack --mode production --display-error-details"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo Load Dynamically App",
                "title": {
                    "en": "Demo Load Dynamically",
                    "de": "Demo dynamisches Laden App"
                },
                "type": "portal-app",
                "bootstrap": "startLoadDynamicallyDemoApp",
                "requires": [
                    "Mashroom Portal Demo React App",
                    "Mashroom Portal Demo Angular App"
                ],
                "category": "Demo",
                "resources": {
                    "js": [
                        "bundle.js"
                    ],
                    "css": []
                },
                "defaultConfig": {
                    "resourcesRoot": "./dist"
                }
            }
        ]
    }
}
