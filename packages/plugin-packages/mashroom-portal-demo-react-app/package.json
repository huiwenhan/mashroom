{
    "name": "@mashroom/mashroom-portal-demo-react-app",
    "description": "Simple React based app that integrates into the Portal",
    "homepage": "https://www.mashroom-server.com",
    "repository": "github:nonblocking/mashroom",
    "license": "MIT",
    "version": "1.4.4",
    "sideEffects": false,
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@mashroom/mashroom-portal": "1.4.4",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    },
    "scripts": {
        "lint": "eslint src",
        "dev": "webpack-dev-server --hot --mode development",
        "build": "webpack --mode production --display-error-details"
    },
    "mashroom": {
        "devModeBuildScript": "build",
        "plugins": [
            {
                "name": "Mashroom Portal Demo React App",
                "title": "Demo React App",
                "type": "portal-app",
                "bootstrap": "startReactDemoApp",
                "category": "Demo",
                "resources": {
                    "js": [
                        "bundle.js"
                    ],
                    "css": []
                },
                "defaultConfig": {
                    "resourcesRoot": "./dist",
                    "restProxies": {},
                    "appConfig": {
                        "firstName": "Jimmy"
                    }
                }
            }
        ]
    }
}
