[ignore]
.*/node_modules/webpack-cli
.*/node_modules/@angular
.*/node_modules/@schematics

[include]

[libs]
flow-typed/

[options]
sharedmemory.hash_table_pow=21
module.ignore_non_literal_requires=true
module.file_ext=.svelte
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'
module.name_mapper='^@mashroom/mashroom\/\(.*\)$' -> '<PROJECT_ROOT>/packages/core/mashroom/\1'
module.name_mapper='^@mashroom/mashroom-utils\/\(.*\)$' -> '<PROJECT_ROOT>/packages/core/mashroom-utils/\1'
module.name_mapper='^@mashroom/mashroom-portal-ui-commons$' -> '<PROJECT_ROOT>/packages/core/mashroom-portal-ui-commons'
module.name_mapper='^@mashroom/mashroom-portal\/\(.*\)$' -> '<PROJECT_ROOT>/packages/plugin-packages/mashroom-portal/\1'
module.name_mapper='^@mashroom/mashroom-security\/\(.*\)$' -> '<PROJECT_ROOT>/packages/plugin-packages/mashroom-security/\1'
module.name_mapper='^@mashroom/mashroom-storage\/\(.*\)$' -> '<PROJECT_ROOT>/packages/plugin-packages/mashroom-storage/\1'
module.name_mapper='^@mashroom/mashroom-messaging\/\(.*\)$' -> '<PROJECT_ROOT>/packages/plugin-packages/mashroom-messaging/\1'
module.name_mapper='^@mashroom/mashroom-i18n\/\(.*\)$' -> '<PROJECT_ROOT>/packages/plugin-packages/mashroom-i18n/\1'
module.name_mapper='^@mashroom/mashroom-browser-cache\/\(.*\)$' -> '<PROJECT_ROOT>/packages/plugin-packages/mashroom-browser-cache/\1'

[lints]
deprecated-utility=off
